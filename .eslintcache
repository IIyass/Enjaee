oneInput\n                    id=\"phone\"\n                    required\n                    name=\"mobile\"\n                    placeholder=\"Mobile Number\"\n                    value={valuePhone}\n                    onChange={setValuePhone}\n                  />\n                  <AuthInput icon=\"lock\" type=\"password\" required placeholder=\"Password\" onChange={handleChange} name=\"password\" value={loginInput.password} />\n                  <div id=\"recaptcha-container\" />\n                  <FooterButton>Login</FooterButton>\n                  <div>\n                    <Link to=\"/signup\">Register here!</Link>\n                    <Link id=\"forgot\" to=\"/forget\">Forgot password</Link>\n                  </div>\n                  {loginError ? <p style={{ textAlign: 'center', color: 'red' }}>{loginError}</p> : null}\n                </form>\n              </div>\n            </Style.Formcontainer>\n\n          </Style.AuthWrapper>\n        );\n\n      case 2:\n        return <OTP />;\n      case 3:\n        return <Share />;\n      default: return null;\n    }\n  };\n\n  return renderAuth();\n};\n\nexport default Login;\n","/home/ilyass/Freelance/Figma_India_Screen/app/src/Components/Profil/index.js",["542"],"import React from 'react';\nimport * as Style from './style';\nimport Bitmap from '../../Illustration/Bitmap.png';\nimport ProfilInput from '../UI/ProfilInput';\nimport ProfilButton from '../UI/ProfilButton';\nimport ProfilSelector from '../UI/ProfilSelector';\nimport ProfilFooter from './ProfilFooter';\n\nconst Profil = () => {\n  const options = [\n    { value: 'abc', label: 'abc' },\n    { value: 'def', label: 'def' },\n  ];\n\n  return (\n    <Style.Wrapper>\n      <h1>Profil</h1>\n      <Style.Hero>\n        <img src={Bitmap} />\n        <Style.InputInfo>\n          <div id=\"ProfilInfo\">\n            <ProfilInput label=\"Name\" placeholder=\"Sandip Dave\" />\n            <ProfilInput label=\"Number\" placeholder=\"+91 (942) 900 0062\" 